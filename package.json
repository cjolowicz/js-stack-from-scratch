    "babel-jest": "^23.6.0",
    "babel-loader": "7",
    "babel-plugin-flow-react-proptypes": "^24.1.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.10.0",
